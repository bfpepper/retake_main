<!DOCTYPE html>
<html>
<head>
  <title>MainApp</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
</head>

<body>
  <% if current_user %>
  <li><%= link_to "Log Out", logout_path%></li>
  <% else %>
  <li><%= link_to "Log In", login_path %> Or <%= link_to 'Sign Up', new_user_path %> </li>
  <% end %>
  
  <%= yield %>
</body>
</html>
